extends default/_wrapper.pug

block content
    header
        .container
            include ../includes/langSelector.pug
        .container.menu-container.menu-container--top
            include ../includes/navigation.pug
    main
        .container.clearfix
            .sidebar.menu-container--sidebar
                include ../includes/navigation.pug
            .content
                h1
                    |#{ssgPrint(pageTitle)}
                h2
                    |#{hello()}

                .js-parallax(
                    data-align='start',
                    data-propierties="opacity")
                    .parallax__item-test

                //- ACCORDION
                h3
                    |accordion default
                div.js-accordion
                    .accrdion-item.js-accordion-item
                        button.accrdion-item__btn.js-accordion-toggle
                            |item1
                        .accrdion-item__content.js-accordion-content(style="height:0;")
                            span
                                |content
                    .accrdion-item.js-accordion-item
                        button.accrdion-item__btn.js-accordion-toggle
                            span
                                |item2
                        .accrdion-item__content.js-accordion-content(style="height:0;")
                            span
                                |content
                    .accrdion-item.js-accordion-item
                        button.accrdion-item__btn.js-accordion-toggle
                            |item2
                        .accrdion-item__content.js-accordion-content(style="height:0;")
                            span
                                |content

                //- ACCORDION
                h3
                    |accordion multiple max desktop
                .js-accordion(
                    data-multiple
                    data-queryType="max"
                    data-breackpoint="desktop")
                    .accrdion-item.js-accordion-item
                        button.accrdion-item__btn.js-accordion-toggle
                            |item1
                        .accrdion-item__content.js-accordion-content(style="height:0;")
                            span
                                |content
                    .accrdion-item.js-accordion-item
                        button.accrdion-item__btn.js-accordion-toggle
                            span
                                |item2
                        .accrdion-item__content.js-accordion-content(style="height:0;")
                            span
                                |content
                    .accrdion-item.js-accordion-item
                        button.accrdion-item__btn.js-accordion-toggle
                            |item2
                        .accrdion-item__content.js-accordion-content(style="height:0;")
                            span
                                |content

                //- ACCORDION
                h3
                    |accordion via js
                .accordion-custom
                    .accrdion-item
                        button.accrdion-item__btn
                            |item1
                        .accrdion-item__content(style="height:0;")
                            span
                                |content
                    .accrdion-item
                        button.accrdion-item__btn
                            span
                                |item2
                        .accrdion-item__content(style="height:0;")
                            span
                                |content
                    .accrdion-item
                        button.accrdion-item__btn
                            |item2
                        .accrdion-item__content(style="height:0;")
                            span
                                |content

                include ../component/lightbox/pug/button.pug

                //- div(data-conponent='m-comp--pageScroller')
                //-     .pageScroller__content
                include ../component/tooltip/pug/tooltip.pug


                .parallax__item-test.js-parallax(
                    data-range='8.5',
                    data-align='center',
                    data-ease
                    data-easeType="lerp")

                .parallax__item-test.js-parallax(
                    data-opacityStart='110'
                    data-opacityEnd='50'
                    data-ease
                    data-easeType="lerp"
                    data-propierties="opacity")

        include ../component/threeBlocks/pug/tBlocks.pug
        include ../component/threeBlocks/pug/tBlocks.pug

        section.overflow-hidden
            .interceptTrigger.parallax__item-test.js-animate(
                data-triggerEl=".interceptObserver-1")
            .interceptObserver.interceptObserver-1.js-animate
            .interceptObserver.interceptObserver-2.js-animate

            .index-timeline(style="border:1px #ccc solid;position: relative;height:140px;")
                //- div(style='position:absolute;left:50%;transform:translateX(-50%)')
                div
                  .index-parallax-1.inlineBLock
                    .parallax__item-test

            .grid-parallax
              .grid-parallax__item.js-parallax(
                  data-type="scrollTrigger"
                  data-propierties="rotate"
                  data-range="45deg"
                  data-start="bottom 50vh -halfHeight"
                  data-end="top +20vh"
                  data-pin
                  data-marker='side-tween-from-top'
                  )
                  .grid-parallax__cell
                    |1
                  .grid-parallax__cell
                    |2

            .flex.justify-center
                .js-parallax(
                    data-perspective="400"
                    data-propierties="rotateX"
                    data-reverse
                    data-ease
                    data-range="6")
                    .js-parallax(
                        data-reverse
                        data-ease
                        data-range="6")
                        .parallax__item-test


            .parallax__item-test.js-parallax(
                data-ease
                data-easeType='spring'
                data-propierties="opacity"
                data-opacityStart="100"
                data-opacityEnd="45")


            .clearfix
                .inlineBLock.js-parallax-scale
                    .parallax__item-test.parallax__item-test--float

            .parallax__item-test.js-parallax(
                data-range='9',
                data-align='90',
                data-onSwitch="in-back",
                data-reverse,
                data-propierties="x")


            .clipath-start.js-toggle-class(
                data-startClass="clipath-start",
                data-endClass="clipath-end")
                    .trasition-item-test

            .flex
                div.js-parallax(
                    2ata-range='6',
                    data-onSwitch="in-stop"
                    data-ease)
                    .parallax__item-test.parallax__item-test--float

                div.js-parallax(
                    data-range='4.5',
                    data-onSwitch="out-stop"
                    data-springConfig="wobbly"
                    data-ease)
                    .parallax__item-test.parallax__item-test--float

            .transformY-start.js-toggle-class(
                data-startClass="transformY-start",
                data-endClass="transformY-end",
                data-onlyOnce)
                    .trasition-item-test

            .flex
                .parallax__item-test.inlineBLock.js-parallax(
                    data-range='9',
                    data-align='bottom',
                    data-ease,
                    data-disableForce3D,
                    data-easeType='spring'
                    data-propierties="rotate")


            .opacity-start.js-toggle-class(
                data-startClass="opacity-start",
                data-endClass="opacity-end")
                    .trasition-item-test

            .parallax__item-test.js-parallax(
                data-range='2',
                data-align='end',
                data-propierties="x")


            div(style="height:200px;")

    include ../component/lightbox/pug/lightbox-simple.pug
    include ../component/lightbox/pug/lightbox-image.pug
    include ../component/lightbox/pug/lightbox-video.pug
