extends default/_wrapper.pug

block content
    header
        .container
            include ../includes/langSelector.pug
        .container.menu-container.menu-container--top
            include ../includes/navigation.pug
    main
        .container.clearfix
            .sidebar.menu-container--sidebar
                include ../includes/navigation.pug
            .content
                h1
                    |#{ssgPrint(pageTitle)}
                h2
                    |#{hello()}

                div(
                    data-conponent='m-comp--parallax',
                    data-align='start',
                    data-propierties="opacity")
                    .parallax__item-test

                //- ACCORDION
                h3
                    |accordion default
                div(data-conponent='m-comp--accordion')
                    .accrdion-item(data-item)
                        button.accrdion-item__btn(data-toggle)
                            |item1
                        .accrdion-item__content(data-content)
                            span
                                |content
                    .accrdion-item(data-item)
                        button.accrdion-item__btn(data-toggle)
                            span
                                |item2
                        .accrdion-item__content(data-content)
                            span
                                |content
                    .accrdion-item(data-item)
                        button.accrdion-item__btn(data-toggle)
                            |item2
                        .accrdion-item__content(data-content)
                            span
                                |content

                //- ACCORDION
                h3
                    |accordion multiple max desktop
                div(data-conponent='m-comp--accordion' 
                    data-multiple
                    data-queryType="max"
                    data-breackpoint="desktop")
                    .accrdion-item(data-item)
                        button.accrdion-item__btn(data-toggle)
                            |item1
                        .accrdion-item__content(data-content)
                            span
                                |content
                    .accrdion-item(data-item)
                        button.accrdion-item__btn(data-toggle)
                            span
                                |item2
                        .accrdion-item__content(data-content)
                            span
                                |content
                    .accrdion-item(data-item)
                        button.accrdion-item__btn(data-toggle)
                            |item2
                        .accrdion-item__content(data-content)
                            span
                                |content
                                
                //- ACCORDION
                h3
                    |accordion via js
                .accordion-custom
                    .accrdion-item
                        button.accrdion-item__btn
                            |item1
                        .accrdion-item__content
                            span
                                |content
                    .accrdion-item
                        button.accrdion-item__btn
                            span
                                |item2
                        .accrdion-item__content
                            span
                                |content
                    .accrdion-item
                        button.accrdion-item__btn
                            |item2
                        .accrdion-item__content
                            span
                                |content

                include ../component/lightbox/pug/button.pug

                include ../component/tooltip/pug/tooltip.pug
                    

                .parallax__item-test(
                    data-conponent='m-comp--parallax',
                    data-range='8',
                    data-align='start',
                    data-ease
                    data-easeType="css")

                .parallax__item-test(
                    data-conponent='m-comp--parallax',
                    data-align='start',
                    data-propierties="opacity")




        include ../component/offset-slider/pug/offsetSlider.pug
        .container.container-block-1
            include ../component/threeBlocks/pug/tBlocks.pug
        .container.container-block-2
            include ../component/threeBlocks/pug/tBlocks.pug

        section.overflow-hidden
            
            .interceptTrigger.parallax__item-test(
                data-conponent='m-comp--animate'
                data-triggerEl=".interceptObserver-1")
            .interceptObserver.interceptObserver-1(
                data-conponent='m-comp--animate')
            .interceptObserver.interceptObserver-2(
                data-conponent='m-comp--animate')
        
            .index-timeline
                .index-timeline__dummy.index-timeline__dummy--1
                .index-timeline__dummy.index-timeline__dummy--2
                div.inlineBLock(
                    data-conponent='m-comp--parallax'
                    data-computationType="fixed"
                    data-reverse
                    data-fixedOffset='30'
                    data-propierties="vertical"
                    data-range="100"
                    data-scrollTrigger=".index-timeline__dummy--1")
                    div(
                        data-conponent='m-comp--parallax'
                        data-computationType="fixed"
                        data-propierties="horizontal"
                        data-reverse 
                        data-fixedOffset='30'
                        data-range="50"
                        data-scrollTrigger=".index-timeline__dummy--2")
                        .parallax__item-test
                            
            .parallax__item-test.inlineBLock(
                data-conponent='m-comp--parallax'
                data-computationType="fixed"
                data-propierties="rotate"
                data-range="90"
                data-fixedOffset="20")
                
                    
            .flex.justify-center
                div(
                    data-conponent='m-comp--parallax'
                    data-perspective="400"
                    data-propierties="rotateX"
                    data-range="6")
                    div(
                        data-conponent='m-comp--parallax'
                        data-range="6")
                        .parallax__item-test


            .parallax__item-test(
                data-conponent='m-comp--parallax',
                data-propierties="opacity"
                data-opacityStart="100"
                data-ease
                data-opacityEnd="45")
                

            .clearfix
                .inlineBLock(
                    data-conponent='m-comp--parallax',
                    data-range='9.5',
                    data-ease
                    data-onSwitch="in-back",
                    data-propierties="scale")
                    .parallax__item-test.parallax__item-test--float

            .parallax__item-test(
                data-conponent='m-comp--parallax',
                data-range='2',
                data-align='90',
                data-onSwitch="in-back",
                data-reverse,
                data-propierties="horizontal")
                

            .clipath-start(
                data-conponent='m-comp--toggleEl',
                data-startClass="clipath-start",
                data-endClass="clipath-end")
                    .trasition-item-test 

            .flex
                div(
                    data-conponent='m-comp--parallax',
                    data-range='8',
                    data-onSwitch="in-stop"
                    data-ease)
                    .parallax__item-test.parallax__item-test--float

                div(
                    data-conponent='m-comp--parallax',
                    data-range='8',
                    data-onSwitch="out-stop"
                    data-ease)
                    .parallax__item-test.parallax__item-test--float

            .transformY-start(
                data-conponent='m-comp--toggleEl',
                data-startClass="transformY-start",
                data-endClass="transformY-end",
                data-onlyOnce)
                    .trasition-item-test 

            .flex
                .parallax__item-test.inlineBLock(
                    data-conponent='m-comp--parallax',
                    data-range='8',
                    data-align='bottom',
                    data-ease,
                    data-propierties="rotate")
                    

            .opacity-start(
                data-conponent='m-comp--toggleEl',
                data-startClass="opacity-start",
                data-endClass="opacity-end")
                    .trasition-item-test 

            .parallax__item-test(
                data-conponent='m-comp--parallax',
                data-range='2',
                data-align='end',
                data-propierties="horizontal")
                

            div(style="height:200px;")

    include ../component/lightbox/pug/lightbox-simple.pug
    include ../component/lightbox/pug/lightbox-image.pug
    include ../component/lightbox/pug/lightbox-video.pug
