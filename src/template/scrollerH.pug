extends default/_wrapper.pug

block content
    header
        .container
            include ../includes/langSelector.pug
        .container.menu-container.menu-container--top
            include ../includes/navigation.pug
    main
        .container.clearfix(style='margin-left:200px;')
            .scrollerH-container
                .scrollerH
                    .scrollerH__item
                    .scrollerH__item
                    .scrollerH__item
                        div(style="display:inline-block;"
                            data-conponent='m-comp--parallax',
                            data-direction='horizontal'
                            data-scroller='.scrollerH-container .scrollerH'
                            data-align='start',
                            data-propierties='x')
                            a(href="https://www.google.com/" target="_blank")
                                |button
                    .scrollerH__item
                        div(style="display:inline-block;"
                            data-conponent='m-comp--parallax',
                            data-direction='horizontal'
                            data-scroller='.scrollerH-container .scrollerH'
                            data-onSwitch='out-stop'
                            data-range='-1.5'
                            data-propierties='x')
                            .div(
                                data-conponent='m-comp--parallax',
                                data-direction='horizontal'
                                data-scroller='.scrollerH-container .scrollerH'
                                data-onSwitch='in-stop'
                                data-range='-6'
                                data-propierties='x')
                                |test
                    .scrollerH__item
                        .parallax-via-js-opacity
                            |test
                    .scrollerH__item
                        .pluto(style='width:200px;height:150px;border:1px #000 solid;text-align:center;')
                            div(style="display:inline-block;border:1px #000 solid;"
                                data-conponent='m-comp--parallax',
                                data-direction='horizontal'
                                data-scroller='.scrollerH-container .scrollerH'
                                data-computationType='fixed'
                                data-scrollTrigger='.pluto'
                                data-propierties='y'
                                data-range='100h'
                                data-start="right +width"
                                data-end="right 50 +width/")
                                |test
                    .scrollerH__item
                        .pluto3(style='width:300px;height:100px;border:1px #000 solid;')
                            .parallax-via-js-in
                                .parallax-via-js-out
                                    div(style="display:inline-block;border:1px #000 solid;")
                                        |test

                    .scrollerH__item
                        
            .scrollerH-container2
                .scrollerH2
                    .scrollerH2__item
                    .scrollerH2__item
                    .scrollerH2__item
                        div(style="display:inline-block;"
                            data-conponent='m-comp--parallax',
                            data-direction='horizontal'
                            data-scroller='.scrollerH-container2 .scrollerH2'
                            data-screen='.scrollerH-container2'
                            data-reverse
                            data-onSwitch='in-stop'
                            data-propierties='x')
                            |test
                    .scrollerH2__item
                        .pluto2(style='width:300px;height:100px;border:1px #000 solid;')
                            div(style="display:inline-block;"
                                data-conponent='m-comp--parallax',
                                data-direction='horizontal'
                                data-scroller='.scrollerH-container2 .scrollerH2'
                                data-screen='.scrollerH-container2'
                                data-computationType='fixed'
                                data-scrollTrigger='.pluto2'
                                data-start="right"
                                data-end='right +width'
                                data-range='80px'
                                data-propierties='x'
                                data-smoothType='linear')
                                |test
                    .scrollerH2__item
                    .scrollerH2__item
                    .scrollerH2__item
            
            .scrollerH-container3
                .scrollerH3
                    .scrollerH3__item
                    .scrollerH3__item
                        .pluto5(style='width:300px;height:100px;border:1px #000 solid;')
                            div(data-conponent='m-comp--parallax',
                                data-scroller='.scrollerH-container3 .scrollerH3'
                                data-screen='.scrollerH-container3'
                                data-computationType='fixed'
                                data-scrollTrigger='.pluto5'
                                data-end='bottom +height'
                                data-propierties="x"
                                data-range='100w')
                                .div(
                                    data-conponent='m-comp--parallax',
                                    data-scroller='.scrollerH-container3 .scrollerH3'
                                    data-screen='.scrollerH-container3'
                                    data-computationType='fixed'
                                    data-propierties="x"
                                    data-scrollTrigger='.pluto5'
                                    data-start='top'
                                    data-end='top +height'
                                    data-fromTo
                                    data-range='-100w')
                                    div(style="display:inline-block;border:1px #000 solid;")
                                        |test
                    .scrollerH3__item
                    .scrollerH3__item
                        div(style="display:inline-block;"
                            data-conponent='m-comp--parallax',
                            data-scroller='.scrollerH-container3 .scrollerH3'
                            data-screen='.scrollerH-container3'
                            data-onSwitch='out-stop'
                            data-propierties='x')
                            |test
                    .scrollerH3__item
                    .scrollerH3__item
                    .scrollerH3__item
                    
                
