<!doctype html>
html
	head
		include pug/head.pug
		
	body
		div(id="spriteSvg" data-svg="assets/svg/svg.svg")
		
		header
			.container.menu-container
				include component/navigation/pug/menu.pug
		main
			.container.clearfix
				.sidebar.menu-container--sidebar
					include component/navigation/pug/menu.pug
				.content
					h1
						|Title
						
					.parallax__container
						.parallax__item.parallax__item-test(data-velocity='3', data-align='start', data-ease="smooth")
					
					include component/lightbox/pug/button.pug
					include component/tooltip/pug/tooltip.pug
					
			.container
				include component/threeGallery/pug/threeGallery.pug
				include component/threeGallery/pug/threeGallery.pug
									
				section.overflow-hidden
					
					.parallax__container
						.parallax__item.parallax__item-test(data-velocity='2', data-align='top', data-ease="smooth", data-direction="horizontal")
							
					.parallax-background__container.homebox.hideBox(style="min-height:300px;")
						.parallax-background__item(data-velocity='3', data-height='1.5', data-align='center', data-ease="smooth")
							img.parallax-background__img(src='assets/img/parallax.jpg')
						|Lorem ipsuum
						
					.parallax__container
						.parallax__item.parallax__item-test(data-velocity='2', data-align='center', data-ease="smooth")

					.parallax-background__container.homebox.hideBox(style="min-height:300px;")
						.parallax-background__item(data-velocity='2', data-ease="linear")
							img.parallax-background__img(src='assets/img/parallax.jpg')
						|Lorem ipsuum 
						
					.parallax__container
						.parallax__item.parallax__item-test(data-velocity='2', data-align='bottom', data-ease="smooth", data-direction="rotate")
						
					.parallax-background__container.homebox.hideBox(style="min-height:300px;")
						.parallax-background__item(data-velocity='14', data-ease="linear")
							img.parallax-background__img(src='assets/img/parallax.jpg')
						|Lorem ipsuum
						
					.parallax__container
						.parallax__item.parallax__item-test(data-velocity='2', data-align='end', data-ease="smooth", data-direction="horizontal")
					div(style="height:200px;")
			

		footer
			include component/lightbox/pug/lightbox-simple.pug
			include component/lightbox/pug/lightbox-image.pug
			include component/lightbox/pug/lightbox-video.pug
			include component/to-top/pug/toTop.pug

		script
			include ../www/assets/js/fontfaceobserver.js
			
		script
			include ../www/assets/js/async-assets-loading.min.js

		script(src="assets/js/vendor/jquery.min.js")
		script(src="assets/js/main.js")
