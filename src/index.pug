<!doctype html>
html
    head
        include pug/head.pug

    body
        div(id="spriteSvg" data-svg="assets/svg/svg.svg")

        header
            .container.menu-container.menu-container--top
                include component-vanilla/navigation/pug/menu.pug
        main
            .container.clearfix
                .sidebar.menu-container--sidebar
                    include component-vanilla/navigation/pug/menu.pug
                .content
                    h1
                        |!{title}

                    .parallax__container
                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-align='start',
                            data-propierties="opacity")
                            .parallax__item-test

                    //- popToggle
                    button.popTogglebtn.popTogglebtn-1
                        |popbtn 1
                    button.popTogglebtn.popTogglebtn-2
                        |popbtn 2
                    button.popTogglebtn.popTogglebtn-3
                        |popbtn 3
                    .popTarget.popTarget-1
                        |popTarget 1
                    .popTarget.popTarget-2
                        |popTarget 2
                        button
                            |close
                    .popTargetDrop.popTarget-3
                        span
                            |popTarget 3
                            button
                                |close
                    include component-vanilla/overlay/pug/overlay.pug
                    //- Fine pop toggle


                    //- ACCORDION
                    .accrdion-item 
                        button.accrdion-item__btn
                            |item1
                        .accrdion-item__content
                            span
                                |content
                    .accrdion-item 
                        button.accrdion-item__btn
                            span
                                |item2
                        .accrdion-item__content
                            span
                                |content
                    .accrdion-item 
                        button.accrdion-item__btn
                            |item2
                        .accrdion-item__content
                            span
                                |content

                    //- FINE ACCORDION

                    include component-vanilla/lightbox/pug/button.pug

                    include component-vanilla/tooltip/pug/tooltip.pug
                    
                    .parallax__container
                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-range='8',
                            data-jsDelta="8",
                            data-align='start',
                            data-ease="smooth"
                            data-smoothType="css")
                            .parallax__item-test
                    .parallax__container
                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-align='start',
                            data-propierties="opacity")
                            .parallax__item-test




            include component-vanilla/offset-slider/pug/offsetSlider.pug
            .container.container-block-1
                include component-vanilla/threeBlocks/pug/tBlocks.pug
            .container.container-block-2
                include component-vanilla/threeBlocks/pug/tBlocks.pug

                section.overflow-hidden
                
                    .index-timeline
                        .index-timeline__dummy.index-timeline__dummy--1
                        .index-timeline__dummy.index-timeline__dummy--2
                        .parallax__container
                            .parallax__item.inlineBLock(
                                data-conponent='m-comp--parallax'
                                data-computationType="fixed"
                                data-propierties="horizontal"
                                data-range="50"
                                data-fixedOffset="20"
                                data-triggerEl=".index-timeline__dummy--1")
                                .parallax__item(
                                    data-conponent='m-comp--parallax'
                                    data-computationType="fixed"
                                    data-propierties="horizontal"
                                    data-reverse
                                    data-range="50"
                                    data-fixedOffset="20"
                                    data-triggerEl=".index-timeline__dummy--2")
                                    .parallax__item-test
                                    
                    .parallax__container
                        .parallax__item.inlineBLock(
                            data-conponent='m-comp--parallax'
                            data-computationType="fixed"
                            data-propierties="rotate"
                            data-range="90"
                            data-fixedOffset="20")
                            .parallax__item-test
                            
                    .parallax__container.flex.justify-center
                        .parallax__item(
                            data-conponent='m-comp--parallax'
                            data-perspective="400"
                            data-propierties="rotateX"
                            data-range="6")
                            .parallax__item(
                                data-conponent='m-comp--parallax'
                                data-range="6")
                                .parallax__item-test

                    .parallax__container
                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-propierties="opacity"
                            data-opacityStart="80"
                            data-opacityEnd="45"
                            data-onSwitch="back")
                            .parallax__item-test

                    .parallax__container.clearfix
                        .parallax__item.inlineBLock(
                            data-conponent='m-comp--parallax',
                            data-range='9.5',
                            data-jsDelta="8",
                            data-ease="smooth",
                            data-onSwitch="back",
                            data-propierties="scale")
                            .parallax__item-test.parallax__item-test--float

                    .parallax__container
                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-range='2',
                            data-align='90',
                            data-ease="linear",
                            data-onSwitch="back",
                            data-reverse,
                            data-propierties="horizontal")
                            .parallax__item-test

                    .transition--clipath.toggleEl--clipath--start(
                        data-conponent='m-comp--toggleEl',
                        data-startClass="toggleEl--clipath--start",
                        data-endClass="toggleEl--clipath--end")
                            .trasition-item-test 

                    .parallax__container.flex
                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-range='8',
                            data-jsDelta="8",
                            data-ease="smooth")
                            .parallax__item-test.parallax__item-test--float

                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-range='8',
                            data-jsDelta="8",
                            data-ease="smooth",
                            data-reverse)
                            .parallax__item-test.parallax__item-test--float
        
                    .transition--translate.toggleEl--transform--start(
                        data-conponent='m-comp--toggleEl',
                        data-startClass="toggleEl--transform--start",
                        data-endClass="toggleEl--transform--end",
                        data-onlyOnce)
                            .trasition-item-test 
        
                    .parallax__container
                        .parallax__item.inlineBLock(
                            data-conponent='m-comp--parallax',
                            data-range='8',
                            data-align='bottom',
                            data-ease="smooth",
                            data-propierties="rotate")
                            .parallax__item-test
        
                    .transition--opacity.toggleEl--opacity--start(
                        data-conponent='m-comp--toggleEl',
                        data-startClass="toggleEl--opacity--start",
                        data-endClass="toggleEl--opacity--end")
                            .trasition-item-test 
        
                    .parallax__container
                        .parallax__item(
                            data-conponent='m-comp--parallax',
                            data-range='2',
                            data-align='end',
                            data-ease="linear",
                            data-propierties="horizontal")
                            .parallax__item-test
        
                    div(style="height:200px;")
        
        include component-vanilla/lightbox/pug/lightbox-simple.pug
        include component-vanilla/lightbox/pug/lightbox-image.pug
        include component-vanilla/lightbox/pug/lightbox-video.pug
        include pug/footer.pug
